@model List<Dictionary<string, object>>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Firebase Data</title>
</head>
<body>
    <h1>Danh Sách Người Dùng</h1>

    <table border="1">
        <thead>
            <tr>
                <th>Tên</th>
                <th>Email</th>
                <th>Tuổi</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user["name"]</td>
                    <td>@user["email"]</td>
                    <td>@user["age"]</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
